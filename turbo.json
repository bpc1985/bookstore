{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "frontend-vue:dev": {
      "cache": false,
      "persistent": true
    },
    "frontend-vue:build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
