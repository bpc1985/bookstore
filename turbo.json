{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "cache": true,
      "outputs": [
        "coverage/**"
      ]
    },
    "test:ui": {
      "cache": false,
      "persistent": true
    },
    "test:run": {
      "cache": true
    },
    "test:coverage": {
      "cache": true,
      "outputs": [
        "coverage/**"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}